<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="database" path="*" />
        <exclude domain="sharedpref" path="auth_prefs.xml" />
        <exclude domain="sharedpref" path="session_prefs.xml" />
        <exclude domain="sharedpref" path="oauth_prefs.xml" />
        <exclude domain="sharedpref" path="user_prefs.xml" />
        <exclude domain="file" path="sensitive_data" />
        <exclude domain="external" path="sensitive_data" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="database" path="*" />
        <exclude domain="sharedpref" path="auth_prefs.xml" />
        <exclude domain="sharedpref" path="session_prefs.xml" />
        <exclude domain="sharedpref" path="oauth_prefs.xml" />
        <exclude domain="sharedpref" path="user_prefs.xml" />
        <exclude domain="file" path="sensitive_data" />
        <exclude domain="external" path="sensitive_data" />
    </device-transfer>
</data-extraction-rules>

