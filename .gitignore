# ================================
# Habitate Android Project .gitignore
# ================================

# ================================
# Android / Gradle
# ================================

# Built application files
*.apk
*.aar
*.ap_
*.aab

# Java class files
*.class

# Generated files
bin/
gen/
out/
output/

# Gradle files
.gradle/
build/
app/build/
**/build/
.kotlin/

# Gradle wrapper (optional - uncomment if you want to include it)
# !gradle/wrapper/gradle-wrapper.jar

# Local configuration file (contains sdk.dir, API keys, etc.)
local.properties

# Proguard folder generated by Eclipse
proguard/

# ================================
# IDE - Android Studio / IntelliJ
# ================================

# IntelliJ project files
*.iml
.idea/
.idea/workspace.xml
.idea/tasks.xml
.idea/gradle.xml
.idea/assetWizardSettings.xml
.idea/dictionaries/
.idea/libraries/
.idea/caches/
.idea/navEditor.xml
.idea/modules.xml
.idea/vcs.xml
.idea/misc.xml
.idea/compiler.xml
.idea/inspectionProfiles/
.idea/runConfigurations/
.idea/kotlinc.xml
.idea/migrations.xml
.idea/shelf/

# Android Studio
.navigation/
captures/
*.hprof

# External native build folder
.externalNativeBuild/
.cxx/

# ================================
# IDE - Visual Studio Code
# ================================

.vscode/
*.code-workspace

# ================================
# Signing & Secrets
# ================================

# Keystore files (NEVER commit these!)
*.jks
*.keystore
keystore/
!keystore/.gitkeep

# Google Services configuration
google-services.json
serviceAccountKey.json

# Environment files
.env
.env.*
*.env

# API keys / secrets
secrets.properties
api-keys.properties

# ================================
# Firebase
# ================================

firebase-debug.log
firebase-debug.*.log
.firebase/
.firebaserc

# Crashlytics
com_crashlytics_export_strings.xml
crashlytics.properties
crashlytics-build.properties
fabric.properties

# ================================
# Testing
# ================================

# Test outputs
/app/testResults/
/app/test-results/
/app/androidTest-results/
*.exec

# Coverage
jacoco/
coverage/
*.ec

# ================================
# Python (for scripts)
# ================================

__pycache__/
*.py[cod]
*$py.class
.venv/
venv/
env/
.Python
pip-log.txt
pip-delete-this-directory.txt

# ================================
# Node.js (if using any JS tooling)
# ================================

node_modules/
npm-debug.log
yarn-error.log
.npm/

# ================================
# OS Generated Files
# ================================

# macOS
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes

# Windows
Thumbs.db
Thumbs.db:encryptable
ehthumbs.db
ehthumbs_vista.db
*.stackdump
[Dd]esktop.ini
$RECYCLE.BIN/

# Linux
*~
.fuse_hidden*
.directory
.Trash-*
.nfs*

# ================================
# Logs & Temporary Files
# ================================

*.log
*.tmp
*.temp
*.swp
*.swo
*.bak
*.orig

# ================================
# Documentation (generated)
# ================================

docs/api/
javadoc/
kdoc/

# ================================
# Miscellaneous
# ================================

# Premium/license checks
/app/src/main/assets/premium.json

# Lint
lint-results*
lint-report*

# APK analysis
*.idsig

# Room schema exports (optional - might want to track these)
# schemas/

# Baseline profiles (binary files)
# app/src/main/baseline-prof.txt

# Typo in original
.gitgub/

# Compose compiler metrics (for debugging)
compose_compiler/
compose-metrics/
compose-reports/
